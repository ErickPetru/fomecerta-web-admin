{
  "name": "fomecerta-web-admin",
  "description": "Sistema Web de administração do app Fome Certa",
  "author": "Erick Eduardo Petrucelli <erick.petrucelli@fatectq.edu.br>",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install:src": "cd src && npm i && cd ..",
    "install:functions": "cd functions && npm i && cd ..",
    "install": "npm run install:src && npm run install:functions",
    "dev": "npm --prefix src run dev",
    "deploy": "firebase deploy",
    "lint": "npm --prefix src run lint",
    "generate": "npm --prefix src run generate",
    "analyze": "npm --prefix src run analyze"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.2",
    "@nuxtjs/eslint-module": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "^1.19.1"
  }
}
